@media only screen and (max-width: 1056px) {
	.gotop {
		right: 0;
	}
	.slider-outer .bx-wrapper img {
		height: 465px
	}
	.slider-outer .banner img {
		height: 200px
	}
	.input-slide {
		width: 38%
	}
	.brnd-h {
		padding-bottom: 75px
	}
	
/*css for ios input */
input{
	-webkit-appearance: none;
	appearance: none;

}

}

@media only screen and (max-width: 992px) {
	.menu ul {
		padding: 0 12px;
	}
	.menu ul li a, .menu ul li.grn-lft a {
		padding: 0 14px;
		background: none
	}
	.input-slide {
		width: 36%
	}
	.deals {
		width: 50%;
	}
	.social {
		float: left
	}
	.hover-eff {
		padding: 15px 5%;
		width: 90%;
		height: 94%
	}
	.hover-eff p {
		font-size: 13px;
		line-height: 16px;
		margin: 0 0 10px;
	}
	.footer_top, .footer_bottom, .top-content, .body_container, .bg-slide, .blue-bg, .breadcrumb {
		width: 98% !important;
		padding-left: 1% !important;
		padding-right: 1% !important
	}
	.save-box img {
		padding: 0
	}
	.footer_col {
		padding-right: 6%;
	}
	.short-pagination ul li.spcl {
		float: left;
	}
	.box-space.box-style .txt-box h2 {
		font-size: 12px;
		font-weight: 600;
	}
	.save-box h2 {
		font-size: 18px;
	}
	.left-container {
		width: 26%;
	}
	.people_head h3 {
		font-size: 24px;
		line-height: 30px;
	}
	.people_head h3 small {
		font-size: 13px;
		line-height: 30px;
	}
	.item_description_L {
		float: none;
		margin: 0 auto 20px;
		overflow: hidden;
	}
	.item_description_R {
		float: none;
		margin: 0 auto;
		width: 390px;
	}
	.shop_details li .items {
		width: 350px;
	}
	.shop_details li .price, .shop_details li .qty, .shop_details li .sub_total {
		width: 130px
	}

	/*dashboard*/
	.account_innerbox {
		width: 96%;
		margin: 0 2%;
	}
	.tablecontent {
		width: 1000px;
	}
	.overflow {
		width: 100%;
		float: left;
		overflow-x: auto;
	}
	.acc_number {
		width: 173px
	}
	.send_payment {
		width: 290px;
	}

	/*edit personal details*/
	.fieldbox {
		margin: 0 40px 5px;
	}

	.map_structure {
		width: 600px
	}

	/*   *********************************   this is to make the scroller styling for iphones and ipads  *****************************************
	 .overflow::-webkit-scrollbar
	 {
	 -webkit-appearance: none;
	 width: 14px;
	 height: 14px;
	 }
	 .overflow::-webkit-scrollbar-thumb
	 {
	 border-radius: 8px;
	 border: 5px solid #fff;
	 background-color: rgba(0, 157, 255, .7);
	 }
	 *                               *******************************************************************************************************       */

	/*order details*/

	.order_details_page .requiredheading small {
		font-size: 10px;
	}
	.date_status p.cont_discription strong {
		width: 100%;
	}
	.date_status p.cont_discription span {
		width: 100%;
		margin-bottom: 10px;
	}

	/*footer*/
	.footer_bottom > .layout > p {
		width: 58%;
	}
	.footer_bottom ul{
		max-width:40%;
	}

}
@media only screen and (max-width: 767px) {
	.contact_inner li, .contact_inner li.last {
		float: left;
		margin: 0 2% 10px;
		width: 46%;
	}
	.social, .subscribe, .deals {
		width: 100%
	}
	.subscribe {
		padding: 13px 0 0 0
	}
	.footer_col, .footer_col.last {
		padding-right: 0;
		width: 50%;
	}
	.deal-h, .brnd-h {
		display: none
	}
	.slider-outer .bx-wrapper img, .slider-outer .banner img {
		height: 300px;
	}
	.bx-controls-direction {
		display: none
	}
	.top-content {
		padding: 65px 0 0;
	}
	.input-slide {
		width: 43%;
		margin: 0 6px 6px 6px;
	}
	.btn-slide {
		width: auto;
		margin-left: 6px;
	}
	.search_sec {
		display: none
	}
	.top-menu {
		float: none
	}
	.top-menu ul {
		width: 380px;
		margin: 0 auto
	}
	.top-menu ul li.ico-ch a {
		border: none
	}
	.menu {
		display: none;
		margin: -34px 2% 0 2%;
		width: 96%
	}
	.menu ul {
		padding: 0
	}
	.menu ul li, .menu ul li.grn-lft {
		float: none;
		display: block;
		border: none
	}
	.menu ul li a {
		line-height: 30px;
	}
	.mobile_menu {
		display: block;
		float: right;
		width: 100%;
		background: #0CBA4E;
		padding: 8px 0;
		width: auto;
		margin: -80px 2% 0 0;
	}
	.mobile_menu a {
		float: left;
		background: url(../images/menu-icon-large.png) no-repeat 0 center;
		width: 40px;
		height: 30px;
		text-indent: -100000px;
		margin-left: 10px
	}
	.mobile_menu a.active {
		background: url(../images/close-icon-large.png) no-repeat 0 center;
	}
	.gotop {
		background-size: 20px auto;
		height: 20px;
		width: 20px;
	}
	.landing_bg .box-style {
		border: none
	}
	.box-style .item_outer {
		width: 332px;
		float: none;
		margin: 0 auto
	}
	.box-style .item_outer .item, .box-style .item_outer.last .item {
		border: 1px solid #E5E5E5;
		margin-bottom: 10px
	}
	.list-big-img h2 {
		font-size: 40px;
		line-height: 45px;
	}
	.list-big-img img {
		min-height: 170px;
	}
	.off-big-img {
		height: 43px;
	}
	.off-big-img h6 {
		height: 43px;
		line-height: 43px;
		font-size: 20px;
	}
	.left-container {
		float: none;
		margin: 0 auto;
		width: 302px;
	}
	.right-container {
		width: 100%;
		margin: 10px 0
	}
	.left-container .left-container-inner {
		padding-left: 5%;
		padding-right: 5%
	}
	.box-space.box-style .item_outer, .box-space.box-style .item_outer.last {
		width: 225px;
		margin: 10px 1%;
		float: left
	}
	.tab_sec {
		margin: 20px 0 0;
	}
	.login_left {
		width: 100%;
	}
	.mob_shop {
		float: left;
		overflow: auto;
		width: 100%;
	}
	.shop_details {
		width: 755px;
	}
	.cupan_sec label {
		padding: 5px 16px 0 0;
		width: auto;
	}
	.cupan_sec {
		width: 380px;
		padding: 20px 0;
	}
	.checkout_outer {
		float: left;
		margin: 0;
	}

	/*dashboard*/
	.contact_info {
		width: 100%;
	}

	.wallet_row {

		min-width: 768px;
	}

	.halfinvoice {
		margin-right: 0;
	}

	/*send payment popup*/

	.sendpaypopup {
		width: 400px;
	}
	.sendpaypopup .divleft, .sendpaypopup .divright {
		width: 100%;
		float: left;
		padding: 0;
		margin-bottom: 6px;
	}
	.sendpaypopup .divright {
		width: 96%;
		padding: 0 2%;
	}

	.map_structure {
		width: 400px
	}
	.map_structure > iframe {
		height: 250px !important;
	}

	/*order details*/

	.halfbox_left, .halfbox_right {
		width: 100%;
		margin-left: 0;
	}

	.user-reg-popup{
width:100%;

}
.user-reg-popup .divleft ,.user-reg-popup .divright{
width:100%;
float:left;
margin-bottom:3px;
box-sizing:border-box;
-webkit-box-sizing:border-box;
-moz-box-sizing:border-box;

}


}

@media only screen and (max-width: 479px) {
	.footer_col, .footer_col.last {
		width: 100%;
	}
	.loc-btn, .cat-btn {
		width: 140px;
		font-size: 14px;
		line-height: 30px;
		height: auto
	}
	.loc-btn span, .cat-btn span {
		background: none;
		padding: 0
	}
	.input-slide {
		width: 91%;
	}
	.btn-slide {
		margin-right: 5px;
		margin-left: 5px;
		font-size: 15px;
	}
	.top-menu ul {
		width: 310px
	}
	.top-menu ul li {
		margin: 0 3px;
		padding: 0;
		background: none
	}
	.top-menu ul li a {
		padding: 0 5px;
		border: none;
		width: auto;
	}
	.footer_bottom {
		text-align: center
	}
	.footer_bottom p {
		float: none
	}
	.footer_bottom > .layout > p, 
	.footer_bottom ul {
		width: 100%;
		max-width:100%;
	}
	.footer_bottom ul li {
		display: inline;
		float: none
	}
	.footer_bottom ul li a {
		display: inline-block;
		float: none
		
	}
	.box-style .item_outer {
		width: 100%;
	}
	.item_description_R {
		width: 310px;
	}
	.item_description_R li .left {
		width: 100px;
	}
	.item_description_R li .right {
		color: #2E3A47;
		float: right;
		width: 205px;
	}
	.review_form li input[type="text"] {
		width: 100%;
		margin: 5px 0 0 0
	}
	.tab_sec li a {
		font-size: 12px;
		padding: 3px 6px;
	}
	.box-space.box-style .item_outer, .box-space.box-style .item_outer.last {
		margin: 0 auto;
		width: 225px;
	}
	.contact_inner li, .contact_inner li.last {
		float: left;
		margin: 0 auto 10px;
		width: 284px;
	}
	.contact_inner li h3 {
		padding: 5px 0;
	}
	.cupan_sec {
		width: 220px
	}
	.cupan_sec label {
		padding: 0 0 10px 0;
		width: 100%;
	}

	/*all pages footer*/

	.footer_col ul {
		padding-left: 2%;
		width: 98%;
	}

	/*dashboard*/

	/*edit_profile-details*/
	.fieldbox {
		margin: 0 16px 5px;
	}
	/*send payment popup*/

	.sendpaypopup#inline1 {
		width: 190px;
	}

	.sendpaypopup .sendpayinstt {
		width: 98%;
		margin-bottom: 10px;
		font-size: 10px;
	}
	.sendpaypopup .sendpayinstt span {
		display: none;
	}

	.map_structure {
		width: 240px
	}

	/*order details*/
	.order_details_page .requiredheading small {
		display: none;
	}

	/*payment*/
	.checkout_form {
		margin-bottom: 35px;
	}

	.checkout_form span, .checkout_form .select_box {
		width: 100%;
		margin-bottom: 25px;
	}
	.checkout_form li {
		padding-bottom: 0;
	}
	.checkout_form li input[type="submit"] {
		margin-top: 0;
	}
	
	
/*checkout*/

.existing_customer .checkout_form li{
	margin-bottom:15px;
	
	
}



