/***Loader CSS Start****/
.loader-holder{
     position:fixed;
     top:0;
     left:0;
     right:0;
     bottom:0;
     background:#fff;
     opacity:0.65;
     z-index:99;
     display:none;
}
.loading .loader-holder{display:block;}
.loader-holder img{
     position:fixed;
     top:50%;
     left:50%;
     margin:-40px 0 0 -40px;
     z-index:100;
}
/***Loader CSS End****/

