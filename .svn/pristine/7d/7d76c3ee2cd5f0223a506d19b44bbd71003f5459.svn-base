/****************reset css starts here*****************/
h1, h2, h3, h4, h5, h6, p, ul, li, body, html, form, fieldset, figure {
	margin: 0;
	padding: 0;
	outline: none;
	font-weight: normal;
	border: 0
}
form, fieldset {
	float: left;
	width: 100%
}
figure {
	float: left
}
button,input{

outline:0px !important;}

img {
	border: 0 none;
	max-width: 100%;
	height: auto;
	float: left
}
a {
	text-decoration: none;
	border: 0;
	outline: 0;
}
ul {
	list-style: none;
}
.clear {
	clear: both;
}

/****************body*****************/
body {
	font-family: 'Open Sans', arial, sans-serif, Helvetica;/*background:url(../images/homepage.jpg) no-repeat center top; opacity:0.3;*/
}
/* layout section start here */
body, html {
	height: 100%;
}
.layout {
	width: 100%;
	max-width: 1000px;
	margin: 0 auto;
}
.header {
	width: 100%;
	float: left
}
.header_top {
	width: 100%;
	background: #f5f5f5;
	min-height: 37px;
	border-bottom: 1px solid #dcdcdc;
	line-height: 35px;
	float: left
}
.top-menu {
	float: right;
}
.top-menu ul {
	list-style-type: none;
	margin: 0px;
	padding: 0px;
}
.top-menu ul  li {
	display: inline-block;
	font-family: 'Open Sans', arial, sans-serif, Helvetica;
	font-size: 12px;
	float: left;
	margin: 0px 10px 0px 0px;
}
.top-menu ul li.last {
	margin-right: 0px;
}
.top-menu ul  li a {
	color: #8c8c8c;
	border-right: 1px solid #dcdcdc;
	padding:0 9px;
}
.ico-hm {
	background: url(../images/top-home.png) no-repeat 0px 11px;
	padding-left: 15px;
}
.ico-ac {
	background: url(../images/top-user.png) no-repeat 0px 12px;
	padding-left: 15px;
}
.ico-sh {
	background: url(../images/top-troly.png) no-repeat 0px 12px;
	padding-left: 15px;
}
.ico-ch {
	background: url(../images/top-check.png) no-repeat 0px 14px;
	padding-left: 15px;
}
.log-btn {
	background: #0fa7bd;
	color: #fff !important;
	width: 63px;
	height: 23px;
	display: inline-block;
	margin-top: 6px;
	text-align: center;
	line-height: 23px;
	border-radius: 3px;
}
.cre-btn {
